{
  "name": "nuxt-custom-fetch",
  "type": "module",
  "version": "4.0.1",
  "packageManager": "pnpm@8.15.9+sha512.499434c9d8fdd1a2794ebf4552b3b25c0a633abcee5bb15e7b5de90f32f47b513aca98cd5cfd001c31f0db454bc3804edccd578501e4ca293a6816166bbd9f81",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xjccc/nuxt-custom-fetch.git"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "echo:husky": "echo 'npx lint-staged' > .husky/pre-commit && echo 'npx --no-install commitlint --edit \"$1\"' > .husky/commit-msg",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "commit": "git-cz",
    "prerelease": "changelogen --prerelease && pnpm prepack && npm publish --tag alpha",
    "release": "changelogen --release && pnpm prepack && npm publish && git push --follow-tags"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vue/shared": "^3.5.17",
    "ohash": "^2.0.11",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nuxt/kit": "4.0.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "4.0.0",
    "@types/node": "^24.0.14",
    "changelogen": "^0.6.2",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nitro": "npm:nitropack@^2.12.3",
    "nuxt": "4.0.0",
    "typescript": "5.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
