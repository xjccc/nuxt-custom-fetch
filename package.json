{
  "name": "nuxt-custom-fetch",
  "type": "module",
  "version": "2.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xjccc/nuxt-custom-fetch.git"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "echo:husky": "echo 'npx lint-staged' > .husky/pre-commit && echo 'npx --no-install commitlint --edit \"$1\"' > .husky/commit-msg",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "commit": "git-cz",
    "release": "changelogen --release && pnpm prepack && npm publish && git push --follow-tags"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.0",
    "ohash": "^2.0.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@types/node": "^22.13.10",
    "changelogen": "^0.6.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nitro": "npm:nitropack",
    "nuxt": "^3.16.0",
    "typescript": "5.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
