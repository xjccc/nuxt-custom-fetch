{
  "name": "nuxt-custom-fetch",
  "type": "module",
  "version": "4.0.0-alpha.2",
  "packageManager": "pnpm@8.15.3+sha1.64838798f519c18029c1e8a1310e16101fc2eda0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xjccc/nuxt-custom-fetch.git"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "echo:husky": "echo 'npx lint-staged' > .husky/pre-commit && echo 'npx --no-install commitlint --edit \"$1\"' > .husky/commit-msg",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "commit": "git-cz",
    "prerelease": "changelogen --prerelease && pnpm prepack && npm publish --tag alpha",
    "release": "changelogen --release && pnpm prepack && npm publish && git push --follow-tags"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "ohash": "^2.0.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@nuxt/kit": "npm:@nuxt/kit-nightly@4.0.0-29086370.088bcd45",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "npm:@nuxt/schema-nightly@4.0.0-29086370.088bcd45",
    "@types/node": "^22.14.1",
    "changelogen": "^0.6.1",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
    "eslint": "^9.24.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "nitro": "npm:nitro-nightly@latest",
    "nuxt": "npm:nuxt-nightly@4.0.0-29086370.088bcd45",
    "typescript": "5.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
